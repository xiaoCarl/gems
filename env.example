# Edit .env and add your LLM API keys

# For DeepSeek (default)
DEEPSEEK_API_KEY=your-deepseek-api-key

# For Qwen3 (optional)
# USE_QWEN3=true
# DASHSCOPE_API_KEY=your-dashscope-api-key
# QWEN3_MODEL=qwen3
# QWEN3_TEMPERATURE=0.7

# 数据源配置
# 实时数据源: 'tdx' (通达信) 或 'akshare' (AkShare)
PREFERRED_DATA_SOURCE=tdx

# 通达信服务器配置（可选）
# TDX_SERVER_IP=115.238.56.198
# TDX_SERVER_PORT=7709
